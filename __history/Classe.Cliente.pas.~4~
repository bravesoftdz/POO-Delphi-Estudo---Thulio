unit Classe.Cliente;

interface

uses Classe.Pessoa;

  type
    TCliente = class(TPessoa)
    private
      FValorCredito : Currency;
      procedure SetValorCredito(const Value: Currency);
    public
      property ValorCredito : Currency read FValorCredito write SetValorCredito;
      function RetornaNome : string; override;
    end;

implementation


end.
